mixin pagination()
	+b.pagination&attributes(attributes)
		+e.w
			+e('a').link._disable._backward(href='#backward')
				| <
			+e('a').link(href='#1') 1
			+e('a').link(href='#1') 2
			+e('a').link(href='#1') 3
			+e('a').link(href='#1') 4
			+e('a').link(href='#1') 5
			+e('a').link._forward(href='#forward')
				| >
